{
  "name": "mycart",
  "author": "Chris Leathley <chris@leathley.ninja>",
  "version": "0.0.1",
  "license": "ISC",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15'",
    "lint": "eslint .",
    "start": "react-native start",
    "start:clear": "react-native start --reset-cache",
    "test": "jest",
    "clean:cache": "yarn cache clean",
    "clean:android": "rm -rf android/build && (cd android && ./gradlew clean)",
    "clean:ios": "rm -rf ios/build ios/Pods ios/Podfile.lock && rm -rf ~/Library/Developer/Xcode/DerivedData",
    "clean:all": "watchman watch-del-all && yarn clean:cache && yarn clean:android && yarn clean:ios && rm -rf yarn.lock node_modules && yarn install && yarn pod:install",
    "pod:install": "(cd ios && bundle install && bundle exec pod install)",
    "upgrade:modules": "yarn upgrade && yarn pod:install",
    "upgrade:check": "yarn outdated",
    "upgrade:react-native": "react-native upgrade",
    "upgrade:all": "yarn upgrade:react-native && yarn upgrade:modules"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.9.8",
    "axios": "^1.3.4",
    "deepmerge": "^4.3.1",
    "i18next": "^23.2.11",
    "react": "18.2.0",
    "react-hook-form": "^7.46.2",
    "react-i18next": "^13.0.2",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-logs": "^5.0.1",
    "react-native-paper": "^5.6.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.21.0",
    "react-native-vector-icons": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.1",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.3",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.2.20",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16"
  }
}
